    "@types/office-runtime": "^1.0.35",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@types/webpack": "^5.28.5",
    "acorn": "^8.11.3",
    "assert": "^2.1.0",
    "autoprefixer": "^10.4.24",
    "babel-loader": "^9.1.3",
