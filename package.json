    "core-js": "^3.36.0",
    "dexie": "^4.3.0",
    "es6-promise": "^4.2.8",
    "lucide-react": "^0.563.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "regenerator-runtime": "^0.14.1",
    "streamdown": "^2.1.0"
  },
